{
  "name": "dhii/delimited-token-template",
  "description": "A flexible implementation for handlebars-style templates",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1 | ^8.0",
    "dhii/output-renderer-interface": "^0.4-alpha1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0 | ^7.0 | ^8.0",
    "psr/container": "^1.0",
    "vimeo/psalm": "^3.11.7 | ^4.0",
    "slevomat/coding-standard": "^6.0",
    "symfony/polyfill-php80": "^1.19"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\Output\\DelimitedTokenTemplate\\": "src"
    }
  },
  "autoload-dev": {
    "Dhii\\Output\\DelimitedTokenTemplate\\Test\\Func\\": "tests/functional"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.2.x-dev"
    }
  }
}
